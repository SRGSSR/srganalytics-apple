{
  "name": "SRGAnalytics",
  "version": "1.5.1",
  "summary": "Analytics for SRG mobile applications",
  "description": "Analytics for SRG mobile applications",
  "homepage": "https://bitbucket.org/rtsmb/srganalytics-ios",
  "license": {
    "type": "N/A"
  },
  "authors": {
    "Cédric Foellmi": "cedric.foellmi@hortis.ch",
    "Cédric Luthi": "cedric.luthi@rts.ch",
    "Frédéric Humbert-Droz": "fred.hd@me.com",
    "Samuel Défago": "defagos@gmail.com",
    "Pierre-Yves Bertholon": "py.bertholon@gmail.com"
  },
  "source": {
    "git": "git@bitbucket.org:rtsmb/srganalytics-ios.git",
    "tag": "1.5.1"
  },
  "requires_arc": true,
  "platforms": {
    "ios": "7.0"
  },
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "source_files": [
        "RTSAnalytics/SRGAnalytics.h",
        "RTSAnalytics/Core/*.{h,m}"
      ],
      "exclude_files": "RTSAnalytics/Core/RTSAnalyticsVersion.m",
      "private_header_files": "RTSAnalytics/Core/*_private.h",
      "frameworks": [
        "AVFoundation",
        "CoreMedia",
        "Foundation",
        "MediaPlayer",
        "UIKit"
      ],
      "dependencies": {
        "ComScore-iOS": [
          "3.1510.231.1"
        ],
        "JRSwizzle": [
          "1.0"
        ]
      },
      "subspecs": [
        {
          "name": "Version",
          "source_files": [
            "RTSAnalytics/Core/RTSAnalyticsVersion.m",
            "RTSAnalytics/Core/RTSAnalyticsVersion_private.h"
          ],
          "compiler_flags": "-DRTS_ANALYTICS_VERSION=1.5.1"
        }
      ]
    },
    {
      "name": "MediaPlayer",
      "source_files": [
        "RTSAnalytics/RTSAnalyticsMediaPlayer.h",
        "RTSAnalytics/MediaPlayer/*.{h,m}"
      ],
      "private_header_files": "RTSAnalytics/MediaPlayer/*_private.h",
      "dependencies": {
        "SRGAnalytics/Core": [

        ],
        "SRGMediaPlayer": [
          "~> 1.9.0"
        ]
      }
    }
  ]
}
